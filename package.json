		"eslint-plugin-react-hooks": "^4.6.0",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"prettier": "2.7.1"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"**/*": "prettier --write --ignore-unknown",
		"*.{ts,tsx}": [
			"eslint --fix"
		]
	}
